          name : BTN_ToastAffiche_avec_une_variable_Toast
          identifier : 0x2eb2df3b0ad85f45
          internal_properties : 
          properties :
           x : 6
           y : 491
           width : 261
           height : 24
          type : 4
          code_elements :
           type_code : 4
           p_codes :
            -
              code : |1+
               
               t		est un toast
               
               // Éléments de base
               nLigne	est un entier
               POUR TOUTE CHAÎNE sLigne de SAI_Message separee par rc
               	t.Texte += [RC] + gPoliceGras(nLigne=1 _ET_ INT_Decor[1]) + ...
               	gPoliceItalique(nLigne>1 _ET_ INT_Decor[2]) + ...
               	gPolice(COMBO_Police[COMBO_Police]) + ...
               	gPoliceTaille(COMBO_TaillePolice[COMBO_TaillePolice]) + ...
               	gStylo(CouleurContraste(SAI_Rouge,SAI_Vert,SAI_Bleu)) + sLigne
               FIN
               
               t.DuréeAffichage	= COMBO_Durée_d_affichage=1 ? toastCourt sinon toastLong
               t.CadrageVertical	= COMBO_Position_verticale-1
               t.CadrageHorizontal	= COMBO_Position_horizontale-1
               t.CouleurFond		= RVB(SAI_Rouge,SAI_Vert,SAI_Bleu)
               
               // Propriétés supplémentaires offertes par le type Toast
               t.FermeAuClic		= INT_OptionsToast[1]
               t.Superposable		= INT_OptionsToast[2]
               t.Titre				= SAI_Titre
               t.Opacité			= POT_Opacité
               
               	procédure interne RetourneClosureBouton( local s est une chaine ) : Procedure
               		procédure interne ClicSurBouton()
               		info("Vous avez cliqué sur le bouton '[%s%]'")
               		fin	
               	p est une procedure = ClicSurBouton
               	renvoyer p
               	fin
               
               
               si COMBO_Boutons>1 ALORS
               	sTexte1 est une chaine = "En savoir plus..."
               	t.Bouton[1].Libellé		= gPolice(COMBO_Police[COMBO_Police])+gStylo(CouleurContraste(SAI_Rouge,SAI_Vert,SAI_Bleu))+sTexte1
               	t.Bouton[1].ActionClic	= RetourneClosureBouton(sTexte1)
               FIN
               si COMBO_Boutons>2 ALORS
               	sTexte2 est une chaine = "Ne plus afficher"
               	t.Bouton[2].Libellé		= gPolice(COMBO_Police[COMBO_Police])+gStylo(CouleurContraste(SAI_Rouge,SAI_Vert,SAI_Bleu))+sTexte2
               	t.Bouton[2].ActionClic	= RetourneClosureBouton(sTexte2)
               FIN
               si COMBO_Boutons>3 ALORS
               	sTexte3 est une chaine = "Détails..."
               	t.Bouton[3].Libellé		= gPolice(COMBO_Police[COMBO_Police])+gStylo(CouleurContraste(SAI_Rouge,SAI_Vert,SAI_Bleu))+sTexte3
               	t.Bouton[3].ActionClic	= RetourneClosureBouton(sTexte3)
               FIN
               
               t.Bouton.CadrageHorizontal = COMBO_Cadrage_horizontal_des_boutons-1
               
               ToastAffiche(t)
