              code : |1-
               // Version 1
               // Description 
               // Sélecteur de couleur avancé
               
               // Fonctionnement : 
               // Permet d'afficher une couleur, en indiquant la couleur hexa Web et la couleur RVB correspondantes
               // Permet également de choisir une couleur : 
               // * via un clic dans la palette de couleurs
               // * en saisissant un valeur hexa Web
               // * en saisissant la valeur RVB de la couleur. 
               
               // Utilisation : 
               // Quelques exemples : 
               // Affecter la couleur sélectionnée
               // <Nom de la variable couleur> = SC_Couleur
               // <Nom du champ>..Couleur = SC_Couleur
               // Initialiser le sélecteur avec une couleur
               // SC_Couleur = MaCouleur
            -
              code : |1+
               
               // Met à jour avec la couleur par défaut
               MajCouleur(VertPastel)
              type : 14
            -
              code : |1+
               
               // Permet la syntaxe SC_Couleur = <une couleur>
               MajCouleur((moimeme..valeur))
              type : 43
            -
              code : |1-
               
               // Permet la syntaxe <une couleur> = SC_Couleur
               CouleurActuelle est une couleur = RVB(SAI_Rouge,SAI_Vert,SAI_Bleu)
               renvoyer CouleurActuelle
              type : 42
           procedures :
            -
              name : MajCouleur
              internal_properties : CAAAAAgAAABow7SSjyw4+RaG603Ketue25DcXFr/Ad4cxtglbfhq10IPQeS5HrR9fDTW0PYi5UrgEIxl/pLZc0yIqc60sDT5I5w6ekQWk6rakv66ZN32ZfbbMd2V3oso5In0dqtU7o8b2vlbrCHtluhTTm00yMEgvZsX2vClYAfwrWz+faFPhs4AIurHJm1IPGlr8TRUte0LUuWYiwA=
              procedure_id : 2212075712728270767
              type_code : 14
              code : |1-
               PROCEDURE MajCouleur(local dCouleur est une couleur = blanc)
               
               // Met à jour l'aperçu de la couleur
               LIB_Couleur..CouleurFond = dCouleur
               
               // Met à jour la valeur hexa HTML de la couleur sans le #
               SAI_HTML = Majuscule(RVBVersHTML(dCouleur)[[2 À]])
               
               // Met à jours la saisie (uniquement si différente, afin de ne pas déplacer le curseur de l'utilisateur)
               si pas SAI_Rouge = dCouleur.Rouge	alors SAI_Rouge  = dCouleur.Rouge
               si pas SAI_Vert  = dCouleur.Vert 	ALORS SAI_Vert   = dCouleur.Vert
               si pas SAI_Bleu  = dCouleur.Bleu 	ALORS SAI_Bleu   = dCouleur.Bleu
              type : 458752
            -
              name : MajDepuisRGB
              procedure_id : 2212075712728336303
              type_code : 14
              code : |1+
               PROCEDURE MajDepuisRGB()
               
               // Convertit la saisie RGB en couleur
               d est une couleur = rgb( val(SAI_Rouge) ,val(SAI_Vert), val(SAI_Bleu) )
               
               // Met à jour l'UI à partir de cette couleur
               MajCouleur(d)
              type : 458752
