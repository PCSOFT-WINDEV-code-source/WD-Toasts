       name : IMG_DessinFuseau
       identifier : 0x2eb3c2a10899aa78
       internal_properties : 
       properties :
        x : 212
        y : 58
        width : 120
        height : 120
       type : 8
       code_elements :
        type_code : 8
        p_codes :
         -
           internal_properties : CAAAAAgAAAB+eToEL5pFuPOuU84it4yv24DRTUS1FB5Zw6dz1T+GT4d4fNmtTZevfgqbGQWi7yQXIP+Ps/XNTHeIelX3iJvDAOui71IG64jdgZoI3ehBxdb9vIW9BX1D/5XH5hS35gnqMVMn15EUHa5H7w==
           code : |1+
            // Choix du fuseau parmi ceux définis par l'IANA Time Zone Database (à changer selon préférence)
            // par ex :
            // Africa/Abidjan
            // Africa/Accra
            // Africa/Addis_Ababa
            // Africa/Algiers
            // Africa/Asmara
            // Africa/Bamako
            // etc
            LIB_Autre_fuseau = <§$0000§>
            
            // Paramètres de dessin
            nRayon 				est un entier = IMG_DessinFuseau..Largeur / 2
            
            // Couleurs des aiguilles
            nCoulSeconde 		est un entier = RGB(220, 56, 43)
            nCoulMinute 		est un entier = RGB(55, 70, 73)
            nCoulHeure 			est un entier = RGB(55, 70, 73)
            nBlancTournant 		est un entier = 20
            
            // Taille et épaisseur des aiguilles
            nTailleHeure 		est un entier = 0.70 * nRayon - nBlancTournant
            nTailleMinute 		est un entier = 0.86 * nRayon - nBlancTournant
            nTailleSeconde 		est un entier = 0.96 * nRayon - nBlancTournant
            nEpaisseurHeure 	est un entier = 5
            nEpaisseurMinute 	est un entier = 3
            nEpaisseurSeconde 	est un entier = 1
            
            // Dessin de l'horloge
            PROCÉDURE INTERNE DessineHorloge
            	
            	// Initialisation du dessin
            	dDébutDessin(IMG_DessinFuseau,dEffacer)
            	dChangeMode(IMG_DessinFuseau,dessinAntiAliasing)	
            	
            	// Heure à dessiner
            	dhDateHeureFuseau est un DateHeure = DateHeureLocaleVersFuseau(DateHeureSys(),LIB_Autre_fuseau)
            	hHeureFuseau est une heure	= dhDateHeureFuseau..PartieHeure
            	IMG_DessinFuseau..Bulle = HeureVersChaîne(hHeureFuseau,"HH:MM:SS")
            	LIB_HeureFuseau = IMG_DessinFuseau..Bulle	
            	
            	// Petite aiguille
            	nAngleAiguille est un entier = ((( (hHeureFuseau..heure modulo 12) * 60) + (hHeureFuseau..Minute)) / 2) - 90
            	dLigne(IMG_DessinFuseau,nRayon + (Cosinus(nAngleAiguille) * nTailleHeure), nRayon + (Sinus(nAngleAiguille) * nTailleHeure), nRayon, nRayon, nCoulHeure, nEpaisseurHeure)
            	
            	// Grande aiguille
            	nAngleAiguille = (((hHeureFuseau..Minute * 60) + (hHeureFuseau..Seconde)) / 10) - 90
            	dLigne(IMG_DessinFuseau,nRayon + (Cosinus(nAngleAiguille) * nTailleMinute), nRayon + (Sinus(nAngleAiguille) * nTailleMinute), nRayon, nRayon, nCoulMinute, nEpaisseurMinute)
            	
            	// Trotteuse
            	nAngleAiguille = (hHeureFuseau..Seconde * 6) - 90
            	dLigne(IMG_DessinFuseau,nRayon + (Cosinus(nAngleAiguille) * nTailleSeconde), nRayon + (Sinus(nAngleAiguille) * nTailleSeconde),nRayon, nRayon, nCoulSeconde, nEpaisseurSeconde)
            	
            FIN
            
            // Dessine l'horloge une première fois
            DessineHorloge()
            
            // Rappelle le dessin d'horloge chaque seconde
            TimerSys(DessineHorloge,1s)
            
